   "source": [
    "from sklearn.linear_model import Perceptron\n",
    "from sklearn.datasets import load_digits\n",
    "\n",
    "data  = load_digits()\n",
    "X = data.data\n",
    "y = data.target\n",
    "\n",
    "modelPtr = Perceptron(tol=1e-3, random_state=42)\n",
    "modelPtr.fit(X,y)\n",
    "modelPtr.score(X,y)"
   ]
  }
